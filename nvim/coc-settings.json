{
  "codeLens.enable": true,
  "coc.preferences.messageLevel": "error",
  "coc.source.emoji.filetypes": [
    "markdown",
    "gitcommit",
    "json",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "gemini"
  ],
  "eslint.autoFixOnSave": true,
  "eslint.fileTypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "diagnostic.hintSign": "\uf684",
  "diagnostic.errorSign": "\uf686",
  "diagnostic.warningSign": "\uf67d",
  "diagnostic.infoSign": "\uf688",
  "diagnostic-languageserver.filetypes": {
    "sh": "shellcheck"
  },
  "git.branchCharacter": "\uf126",
  "git.realtimeGutters": true,
  "git.changedSign.text": "\uf45a",
  "git.changeRemovedSign.text": "\uf459",
  "git.addedSign.text": "\uf457",
  "git.removedSign.text": "\uf458",
  "prettier.disableSuccessMessage": true,
  // README: https://github.com/neoclide/coc.nvim/wiki/Language-servers
  "languageserver": {
    "elixirLS": {
      "command": "~/.config/nvim/language-servers/elixir-ls-1.10.4/language_server.sh",
      "filetypes": [
        "elixir",
        "eelixir"
      ]
    }
  }
}
